{
    "name": "thecoati/surf",
    "description": "Docker files for running a basic Radicle application..",
    "keywords": ["docker", "sail", "radicle"],
    "homepage": "https://roots.io/radicle/",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/TheCoati/Radicle-Surf/issues",
        "source": "https://github.com/TheCoati/Radicle-Surf"
    },
    "authors": [
        {
            "name": "TheCoati",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0",
        "nunomaduro/termwind": "^1.14",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "laravel-zero/framework": "^9.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "resources/database/factories/",
            "Database\\Seeders\\": "resources/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/surf"]
}
